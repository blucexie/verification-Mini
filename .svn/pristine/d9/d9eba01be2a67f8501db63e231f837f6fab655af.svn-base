body {
  font-family: "Microsoft YaHei";
  position: relative;
  background: #f8f8f8;
}
input:disabled,
input[disabled],
textarea:disabled {
  color: #666;
  -webkit-text-fill-color: #666;
  -webkit-opacity: 1;
  opacity: 1;
}
span {
  font-size: 0.6rem;
  color: #333;
}
::-webkit-input-placeholder {
  font-size: 0.56rem;
  color: #ccc;
}
input:focus {
  color: #666;
  caret-color: #666;
}
textarea {
  border-radius: 0;
  /* resize:none;*/
}
input {
  border-radius: 0;
  caret-color: #574e5f;
  -webkit-appearance: none;
  background: none;
  outline: none;
  border: none;
  line-height: normal;
  height: 1.6rem;
  font-size: 0.56rem;
}
#header {
  width: 15rem;
  height: 7.76rem;
  background: url("../images/header-bg.png") no-repeat;
  background-size: 15rem 6.92rem;
}
#logo {
  float: left;
  width: 0.64rem;
  height: 0.74rem;
  margin-left: 0.6rem;
  margin-top: 0.64rem;
}
#logo img {
  width: 0.64rem;
  height: 0.74rem;
}
.company {
  display: inline-block;
  width: 12.4rem;
  padding-top: 0.76rem;
  margin-left: 0.56rem;
}
.company .companyName {
  font-size: 0.6rem;
  /*height:56/@r ;*/
}
.company p {
  font-size: 0.6rem;
  color: #fff;
  line-height: 1.08rem;
}
.company p:nth-of-type(2) {
  text-indent: 2em;
  font-size: 0.56rem;
  line-height: 0.88rem;
}
.warn {
  margin-left: 2.04rem;
  margin-top: 0.28rem;
}
.warn p {
  color: #feee5f;
  font-size: 0.52rem;
  height: 0.76rem;
  line-height: 0.76rem;
  padding-left: 1.08rem;
}
.warn p img {
  width: 0.24rem;
  height: 0.24rem;
  vertical-align: 0.08rem;
}
.licensedContent {
  width: 13.88rem;
  padding-bottom: 0.52rem;
  margin: 0 auto;
  margin-top: -2.2rem;
  background: #fff;
  border-radius: 0.1rem;
  margin-bottom: 0.44rem;
}
.licensedContentTitle {
  display: flex;
  align-items: center;
  padding-top: 0.36rem;
}
.licensedContentTitle h2 {
  font-size: 0.6rem;
  font-weight: 400;
  color: #333;
}
.licensedContentTitle > img {
  float: left;
  width: 0.1rem;
  height: 0.62rem;
  margin: 0 0.24rem;
}
.licensedContent ul li {
  float: left;
  width: 33.33%;
  text-align: center;
  margin-top: 0.4rem;
}
.licensedContent ul li img {
  width: 1.46rem;
  height: 1.46rem;
}
.licensedContent ul li p {
  font-size: 0.52rem;
  color: #333;
}
.interval2 {
  margin-top: 1.12rem;
}
/*基本信息*/
.basic {
  width: 13.88rem;
  background: #fff;
  margin: 0 auto;
  text-align: center;
}
.basicForm label {
  width: 13.24rem;
  font-size: 0.6rem;
  color: #573e5f;
  height: 2rem;
  line-height: 2rem;
  display: inline-block;
  border-bottom: 1px solid #e7e7e7;
  background: #fff;
  margin-left: 0.32rem;
}
.basic img {
  width: 5.36rem;
  height: 1.16rem;
  margin-top: 0.54rem;
}
#mesCodeLabel {
  width: 9.3rem;
  border-bottom: none;
}
#deForm {
  width: 13.88rem;
  margin: 0 auto;
  background-color: #fff;
}
#deForm > div {
  width: 13.88rem;
  float: left;
  display: flex;
  align-items: center;
  background: #fff;
}
#deForm label {
  float: left;
  display: flex;
  align-items: center;
}
#deForm label i {
  float: left;
  width: 0.16rem;
  height: 0.16rem;
  border-radius: 50%;
  margin: 0 0.44rem 0 0.28rem;
}
#deForm .icon1 {
  background: #00a2ea;
}
#deForm .icon2 {
  background: #f22ac6;
}
#deForm .icon3 {
  background: #00aeb4;
}
#deForm .icon4 {
  background: #ffaf3f;
}
#deForm label span {
  margin-right: 2.32rem;
}
.inpId {
  margin-right: 0.4rem !important;
}
.school {
  margin-right: 1rem !important;
}
.salary,
.jobInterview,
.inpTel {
  margin-right: 1rem !important;
}
#validateCode {
  width: 4.8rem;
}
#deForm .msCode {
  margin-right: 0.4rem;
}
#deForm .mesBtn {
  display: inline-block;
  width: 3.72rem;
  height: 1.16rem;
  line-height: 1.16rem;
  margin-left: 0.16rem;
  border: 1px solid #03aaad;
  border-radius: 0.2rem;
  color: #03aaad;
  font-size: 0.52rem;
  background: #fff;
  outline: none;
}
#deForm label input {
  border: none;
  height: 1.6rem;
  outline: none;
  font-size: 0.56rem;
  color: #666;
}
.nextStep {
  text-align: center;
  padding: 1.08rem 0;
  background-color: #fff;
  margin-bottom: 3.6rem;
}
.nextStep button {
  width: 9rem;
  height: 1.48rem;
  background: #00adb4;
  font-size: 0.68rem;
  color: #fff;
  border: none;
  border-radius: 1rem;
  outline: none;
}
.errorShow {
  border-bottom: 1px solid #00adb4!important;
}
.autograph {
  width: 13.88rem;
  margin: 0 auto;
  background-color: #fff;
}
.autograph span {
  margin-left: 0.4rem;
}
.nameSpace {
  width: 12.12rem;
  text-align: center;
  padding: 0.44rem 0;
  border-bottom: 1px solid #e7e7e7;
  margin: 0 auto;
}
.nameSpace > p {
  color: #fc9921;
  font-size: 0.64rem;
  font-style: oblique;
}
.nameSpace img {
  width: 4.8rem;
  height: 1.6rem;
}
/*loading*/
#loading {
  position: fixed;
  top: 40%;
  left: 50%;
  margin-left: -36px;
  z-index: 9992;
  display: none;
}
@-webkit-keyframes line-scale {
  0% {
    -webkit-transform: scaley(1);
    transform: scaley(1);
  }
  50% {
    -webkit-transform: scaley(0.4);
    transform: scaley(0.4);
  }
  100% {
    -webkit-transform: scaley(1);
    transform: scaley(1);
  }
}
@keyframes line-scale {
  0% {
    -webkit-transform: scaley(1);
    transform: scaley(1);
  }
  50% {
    -webkit-transform: scaley(0.4);
    transform: scaley(0.4);
  }
  100% {
    -webkit-transform: scaley(1);
    transform: scaley(1);
  }
}
.line-scale > div:nth-child(1) {
  -webkit-animation: line-scale 1s 0.1s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
  animation: line-scale 1s 0.1s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
}
.line-scale > div:nth-child(2) {
  -webkit-animation: line-scale 1s 0.2s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
  animation: line-scale 1s 0.2s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
}
.line-scale > div:nth-child(3) {
  -webkit-animation: line-scale 1s 0.3s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
  animation: line-scale 1s 0.3s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
}
.line-scale > div:nth-child(4) {
  -webkit-animation: line-scale 1s 0.4s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
  animation: line-scale 1s 0.4s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
}
.line-scale > div:nth-child(5) {
  -webkit-animation: line-scale 1s 0.5s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
  animation: line-scale 1s 0.5s infinite cubic-bezier(0.2, 0.68, 0.18, 1.08);
}
.line-scale > div {
  background-color: #019fe6;
  width: 4px;
  height: 35px;
  border-radius: 2px;
  margin: 2px;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  display: inline-block;
}
.zhezhao {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #000;
  opacity: 0.6;
  display: none;
  z-index: 9000;
}
#clearBtn,
#saveBtn,
#closeBtn {
  background: #fff;
  border: 1px solid #ccc;
  padding: 6px 20px;
  border-radius: 4px;
  line-height: 1.42857143;
}
.htmleaf-container {
  display: none;
  z-index: 9999;
  position: fixed;
  top: 0;
}
